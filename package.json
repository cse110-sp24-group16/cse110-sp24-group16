{
  "name": "Developer-Journal",
  "version": "1.0.0",
  "main": "source/main.js",
  "author": "Team 16",
  "license": "GPL-3.0-only",
  "description": "Journaling app for developers",
  "devDependencies": {
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@electron/packager": "^18.3.2",
    "@eslint/js": "^9.2.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.6",
    "@wdio/cli": "^8.38.2",
    "@wdio/local-runner": "^8.38.2",
    "@wdio/mocha-framework": "^8.38.2",
    "@wdio/spec-reporter": "^8.38.2",
    "electron": "^30.0.9",
    "electron-packager": "^17.1.2",
    "eslint": "^9.4.0",
    "globals": "^15.2.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.3",
    "mocha": "^10.4.0",
    "nyc": "^15.1.0",
    "puppeteer-core": "^22.10.0",
    "puppeteer-in-electron": "^3.0.5",
    "semantic-release": "^24.0.0",
    "wdio-electron-service": "^6.6.1"
  },
  "scripts": {
    "test": "npm run wdio",
    "e2e": "npm run wdio",
    "lint": "eslint",
    "jsdoc": "node --experimental-vm-modules node_modules/jsdoc/jsdoc.js -r ./source/",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-all": "npm run make-win && npm run make-linux && npm run make-darwin",
    "make-all-linux": "npm run make-linux-arm64 && npm run make-linux-x64",
    "make-all-win": "npm run make-win-arm64 && npm run make-win-x64",
    "make-all-darwin": "npm run make-darwin-arm64 && npm run make-darwin-x64",
    "make-win-x64": "npm run make -- --platform=\"win32\" --arch=\"x64\"",
    "make-win-arm64": "npm run make -- --platform=\"win32\" --arch=\"arm64\"",
    "make-linux-x64": "npm run make -- --platform=\"linux\" --arch=\"x64\"",
    "make-linux-arm64": "npm run make -- --platform=\"linux\" --arch=\"arm64\"",
    "make-darwin-x64": "npm run make -- --platform=\"darwin\" --arch=\"x64\"",
    "make-darwin-arm64": "npm run make -- --platform=\"darwin\" --arch=\"arm64\"",
    "wdio": "nyc wdio run ./wdio.conf.js"
  },
  "jest": {
    "transform": {},
    "verbose": true
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "node": "^18.20.3",
    "wdio": "^6.0.1"
  }
}
