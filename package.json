{
  "name": "Developer-Journal",
  "version": "1.0.0",
  "main": "source/main.js",
  "author": "Team 16",
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.6",
    "electron": "^30.0.9",
    "eslint": "^9.4.0",
    "globals": "^15.2.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.3",
    "mocha": "^10.4.0",
